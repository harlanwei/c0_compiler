/**
 * A test function.
 */
const int GLOBALCONST = 3;

int fn(int i) {
    int value = 3;
    int limit = 10;

    while (i < limit) {
        print(i, i * 2);
        i = i + 1;
        value = value + 4;
        print(value);
    }

    return value;
}

void main() {
    int result = fn(GLOBALCONST);
    print(result);

    const int anotherResult = 1 + 2 * 3;
    print(anotherResult);
    
    return;
}
